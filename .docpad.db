tpadding: 1em 1.5em\n\tborder: 1px solid hsl(192, 8%, 84%)\n\n\tline-height: 1.5em\n\n\toverflow: auto\n\t// word-wrap: break-word\n\t// white-space: pre-wrap\n\n\n// ====================================\n// Global Styles\n\nbody\n\tbackground: colorBackground\n\tcolor: colorDarkestBrown\n\nblockquote\n\tmargin: 3em\n\tcolor: hsl(11, 90%, 55%)\n\tem\n\t\tfont-style: italic\n\nem\n\t// color: colorAccent\n\t// font-style: normal\n\tfont-style: italic\n\nnav\nnav li\n\tlist-style: none\n\n.btn\n\tbackground-color: colorLightGold\n\tbackground-image: linear-gradient(top, colorLightGold, colorGold)\n\tborder: 2px solid white\n\tborder-radius: 3px\n\tcolor: colorDarkestBrown !important\n\tdisplay: inline-block\n\tmargin: 1em auto\n\tpadding: 15px 30px\n\ttext-shadow: 1px 1px 0 rgba(white, .3)\n\n\t&:hover\n\t\tbackground-image: none\n\t\tbox-shadow: inset 1px 1px 2px rgba(black, .2)\n\n.contributors\n\t.contributor\n\t\t.contributor-repos\n\t\t\tdisplay: none\n\t\t&:hover\n\t\t\t.contributor-repos\n\t\t\t\tdisplay: inline\n\t\t.contributor-repo\n\t\t\t&:after\n\t\t\t\tcontent: ', '\n\t\t\t&:last-child:after\n\t\t\t\tcontent: ''\n\n\n// ====================================\n// Layout and structure\n\n.container\n\tmargin: 0 auto\n\twidth: 90%\n\tposition: relative\n\tclear: both\n\n.bottombar\n\tmargin-top: 3em\n\twidth: 100%\n\tmin-height: 4em\n\n.topbar\n\tposition: relative\n\ttop: 0\n\tleft: 0\n\twidth: 100%\n\theight: 2.5em\n\tz-index: 501\n\n\tbackground: hsl(0, 0%, 95%)\n\tborder-bottom: 1px solid colorLightBrown\n\tbox-shadow: 0 1px 0 white\n\n\t.topnav\n\t\theight: 2.5em\n\t\tword-wrap: break-word\n\t\toverflow: hidden\n\n.sidebar\n\tposition: fixed\n\ttop: 2.5em\n\tleft: -200px\n\twidth: 220px\n\tpadding-right: 20px\n\tz-index: 500\n\tword-wrap: break-word\n\ttransition: all 0.3s ease-in\n\n\t.list-menu\n\t\toverflow: scroll\n\t\twidth: 200px\n\t\tbackground: hsl(0, 0%, 95%)\n\t\tborder-right: 1px solid colorLightBrown\n\t\tbox-shadow: 0 1px 0 white\n\n.page\n\twidth: 100%\n\n.mainbar\n\tfont-weight: 400\n\tclearfix()\n\n.webchat\n\tposition: fixed\n\tleft: 0\n\tbottom: 0\n\n\twidth: 100px\n\theight: 25px\n\n\tcursor: pointer\n\tborder: 1px solid hsl(209, 32%, 82%)\n\tbackground: hsl(209, 70%, 94%)\n\ttext-align: center\n\tpadding-top: 0.3em\n\nhtml.no-touch\n\t.topbar\n\t\tposition: fixed\n\t.mainbar\n\t\tmargin-top: 40px\n\t.page\n\t\tpadding-top: 2em\n\n\nhtml.no-touch .topbar:hover .sidebar\n.topbar .sidebar.active\n\tleft: 0\n\topacity: 1\n\n\n// ====================================\n// Main navigation\n\n.topbar\n\t.search\n\t.links\n\t.links a\n\t\tdisplay: inline-block\n\n\t.links\n\t\tfloat: left\n\t\tmargin-top: 0.6em\n\t\ta\n\t\t\tline-height: 1em\n\t\t\tfont-size: 1em\n\t\t\tmargin-right: 20px\n\t\t\tcolor: colorDarkestBrown\n\t\t\tpadding-bottom: 2px\n\t\t\tborder-bottom: 6px solid transparent\n\n\t\t\t&:hover\n\t\t\t&.active\n\t\t\t\tborder-color: colorLightGold\n\t\t\t\ttext-decoration: none\n\n\t\ta.logo\n\t\t\tmargin-left: 10px\n\t\t\tfont-size: 1.25em\n\t\t\tpadding-bottom: 1px\n\n\t.search\n\t\tfloat: right\n\t\twidth: 25%\n\t\tmargin-top: 0.3em\n\t\tmargin-right: 0.5em\n\t\t*\n\t\t\tbox-sizing: content-box\n\t\t.gsc-control-cse\n\t\t\tpadding: 0\n\t\t\tborder: none\n\t\t\tbackground: transparent\n\n\n// ====================================\n// Splash Styles\n\n.splash\n.error\n\tsection\n\t\tmargin: 3em 0\n\t\tpadding: 1.5em 0\n\t\ttext-align: center\n\n\th1\n\t\tfont-size: 4em\n\th2\n\t\tfont-size: 2.5em\n\th3\n\t\tfont-size: 2.5em\n\n.error\n\tsection\n\t\tmargin: 0\n\n\th1\n\t\tmargin-bottom: 0\n\n\t#goog-fixurl\n\t\t.other-things\n\t\t\tmargin: 2em 0 0\n\t\t\tfont-weight: normal !important\n\n\t\tul\n\t\tul li\n\t\t\tlist-style: none\n\n.splash\n\tp\n\t\tcolor: colorDarkBrown\n\t\tfont-size: 1.1em\n\t\tline-height: 1.6em\n\t\tmargin-left: auto\n\t\tmargin-right: auto\n\t\tmax-width: 50%\n\n\t.btn\n\t\tfont-size: 1.5em\n\n\n\t// ====================================\n\t// Splash - Hero Space\n\n\t.overview\n\t\tfont-family: fontFutura\n\t\tfont-size: 1.4em\n\t\tline-height: 1.2em\n\t\tlist-style: none\n\t\tpadding: 3em 0 1em\n\t\twrapper(100%)\n\n\t\tli\n\t\t\tcolumn(5)\n\t\t\tpadding: 1em 0\n\t\t\tposition: relative\n\n\t\t\tspan\n\t\t\t\tdisplay: block\n\n\t\t\t.image\n\t\t\t\tdisplay: block\n\t\t\t\tbackground: url(../images/splash-features.png) no-repeat 4px 100%\n\t\t\t\twidth: 152px\n\t\t\t\theight: 256px\n\t\t\t\tmargin: 0 auto\n\n\t\t\t&.versioning .image\n\t\t\t\tbackground-position: 0 10px\n\t\t\t&.skeletons .image\n\t\t\t\tbackground-position: -206px 0\n\t\t\t&.language .image\n\t\t\t\tbackground-position: -400px 0\n\t\t\t&.deploys .image\n\t\t\t\tbackground-position: -596px 0\n\t\t\t&.plugins .image\n\t\t\t\tbackground-position: -806px 0\n\n\t\t\t// Popover\n\t\t\t.popover\n\t\t\t\tposition: absolute\n\t\t\t\tleft: 0\n\t\t\t\tz-index: 3\n\n\t\t\t\tbackground: white\n\t\t\t\tpadding: .25em\n\t\t\t\tborder-radius: .25em\n\n\t\t\t\tline-height: 1.25em\n\n\t\t\t\tvisibility: hidden\n\t\t\t\topacity: 0\n\t\t\t\ttransition: all .2s ease-in-out\n\n\t\t\t.popover:before\n\t\t\t\tcontent: \"\"\n\t\t\t\tbackground: white\n\n\t\t\t\tz-index: -3\n\t\t\t\twidth: 1em\n\t\t\t\theight: 1em\n\t\t\t\tmargin-left: -.5em\n\n\t\t\t\tposition: absolute\n\t\t\t\ttop: -.4em\n\t\t\t\tleft: 50%\n\n\t\t\t\ttransform: rotate(45deg)\n\n\t\t\t&:hover\n\t\t\t\tcursor: pointer\n\n\t\t\t\t.popover\n\t\t\t\t\tvisibility: visible\n\t\t\t\t\topacity: 1\n\t\t\t\t\tmargin-top: 1em\n\n\n\t// ====================================\n\t// Splash - Support & About Sections\n\n\t.support\n\t\tbackground: colorLightGold\n\t\tp\n\t\t\tcolor: colorDarkestBrown\n\n\t.providers\n\t\tlist-style: none\n\t\tpadding: 2em 0 0\n\n\t\tli\n\t\t\tdisplay: inline-block\n\t\t\tmargin: 0 3em 2em\n\n\t\ta\n\t\t\tcolor: colorLightestGold\n\t\t\tfont-size: 1.4em\n\t\t\tfont-weight: 400\n\n\t\t\t&:hover\n\t\t\t\tcolor: white\n\n\t.service-buttons\n\t\tmargin-top: 1.5em\n\n\t.social-buttons\n\t.service-badges\n\t\tdisplay: block\n\t\tmargin-top: 0.5em\n\n\t.service-button\n\t\tdisplay: inline-block\n\t\twidth: 100px\n\t\ttext-align: left\n\t\toverflow: hidden\n\t\t&.google-plus-one-button\n\t\t&.hacker-news-submit-button\n\t\t\twidth: 80px\n\t\t&.twitter-follow-button\n\t\t&.fury-button\n\t\t\twidth: 130px\n\t\t&.reddit-submit-button\n\t\t\twidth: 140px\n\t\t\tiframe\n\t\t\t\tpadding-top: 3px\n\n\t.tweets\n\t\tmargin-top: 2em\n\t\tmargin-bottom: 0\n\n\t.about\n\t\tpadding-bottom: 0\n\t\tmargin-bottom: 0\n\n// ====================================\n// Navigation\n\n// Menu\n.list-menu\n\t// Group\n\tul\n\t\tpadding: 0\n\t\tmarign: 0\n\n\t// Title\n\t.list-menu-title\n\t\tfont-size: 1em\n\t\tmargin: 0\n\t\tfont-weight: inherit\n\t\tfont-family: inherit\n\t\ttext-transform: inherit\n\n\t// Link\n\t.list-menu-link\n\t\tborder-left: 5px solid transparent\n\t\tcolor: colorDarkestBrown\n\t\tdisplay: inline-block\n\t\tpadding: 0.15em 0.5em\n\t\tfont-weight: fontMyraidBold\n\n\t// Category\n\t.list-menu-category > .list-menu-link .list-menu-title\n\t\tfont-weight: fontMyraidBoldest\n\n\t// Item\n\t.list-menu-item .list-menu-link\n\t\ttext-transform: none\n\t\t&:hover\n\t\t\tcolor: colorDarkGold\n\n\t// Item Active\n\t.list-menu-item.active .list-menu-link\n\t\tfont-weight: fontMyraidBold\n\t\tcolor: colorDarkGold\n\n// Sidebar\n.sidebar\n\t// Menu\n\t.list-menu\n\t\tpadding-bottom: 1em\n\n\t\t// Category Group\n\t\t.list-menu-categories\n\t\t\tmargin: 0.3em 0\n\t\t\tborder-left: 2px solid colorLightGold\n\n\t\t// Category\n\t\t.list-menu-category\n\t\t\tpadding: 5px 0\n\n\t\t\t&.active > .list-menu-link\n\t\t\t\tborder-color: colorLightGold\n\t\t\t/*\n\t\t\t&.inactive\n\t\t\t\t.list-menu-items\n\t\t\t\t\tmax-height: 0\n\t\t\t&:hover\n\t\t\t\t.list-menu-items\n\t\t\t\t\tmax-height: 300px\n\t\t\t*/\n\n\t\t// Item Group\n\t\t.list-menu-items\n\t\t\tmargin: 5px 0\n\t\t\tpadding-left: 1em\n\t\t\toverflow: hidden\n\t\t\ttransition: max-height 0.5s ease-out\n\t\t\theight: auto\n\t\t\tmax-height